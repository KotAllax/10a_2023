program Picture;
uses GraphABC;
begin;

rectangle(0,0,330,330);
rectangle(30,30,290,290);

DrawCircle(290,30,20); 
DrawCircle(30,150,20);
DrawCircle(200,180,30);
DrawCircle(140,130,30);

line(30,30,100,80);
line(30,65,90,100);
line(100,80,90,100);

Floodfill(10,10,clblack);
Floodfill(289,29,clblack);
Floodfill(29,149,clblack);
Floodfill(35,35,clblack);
Floodfill(200,180,clblack);
Floodfill(140,130,clblack);

end.
