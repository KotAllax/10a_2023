var b,c,h,s,MA,MB,BC,n,m:real;
begin
 readln(s,h,b);
 writeln('Длина стороны c = ', (s*2)/h);
  writeln('Дина МА = ', sqrt(sqr(b)-sqr(h)));
  readln(c,MA);
   writeln('Длина MB = ',c - MA);
   readln(MB);
    writeln('Сторона BC = ', sqrt(sqr(MB)+(sqr(h))):2:1);
    readln(BC);
     writeln('Периметр треугольника = ', bc+c+b);
      writeln('Угол альфа = ', sin(b/h)*60:3:1);
      writeln('Угол бета = ', sin(b/bc)*60:3:1);
      readln(n,m);
      writeln('Угол гамма = ', 180-n-m);
end.
